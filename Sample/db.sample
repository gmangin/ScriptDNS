; BIND reverse data file for local loopback interface
;
$TTL	3600
@	IN	SOA	__DOMAIN_NAME__. root.__DOMAIN_NAME__. (
			      1 	; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 86400 )	; Negative Cache TTL
;
@	IN	NS	ns1.__DOMAIN_NAME__.
@	IN	NS	ns?.__DOMAIN_NAME__.
@	IN	A	__IP_SERVER__
ns1	IN	A	__IP_SERVER__
ns?	IN	A	__SLAVE__

;Hostname entries
__SUB_NAME_	IN	A	_SUB_IP_
